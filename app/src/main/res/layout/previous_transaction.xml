<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/previous_iv"
        android:layout_width="match_parent"
        android:layout_height="151dp"
        android:padding="4dp"
        android:scaleType="fitXY"
        android:src="@drawable/rectangle27"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:layout_width="40dp"
        android:layout_height="35dp"
        android:layout_marginStart="6dp"
        android:layout_marginBottom="12dp"
        android:src="@drawable/basket"
        app:layout_constraintBottom_toBottomOf="@+id/previous_iv"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:layout_width="30dp"
        android:layout_marginTop="8dp"
        android:layout_marginRight="48dp"
        android:layout_height="35dp"
        android:src="@drawable/cherry2"
        app:layout_constraintEnd_toStartOf="@+id/previous_search"
        app:layout_constraintTop_toBottomOf="@+id/previous_tv" />

    <ImageView
        android:layout_width="40dp"
        android:layout_height="30dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="8dp"
        android:src="@drawable/cake"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/previous_search" />

    <ImageView
        android:layout_width="30dp"
        android:layout_height="40dp"
        android:layout_marginStart="64dp"
        android:src="@drawable/fries"
        app:layout_constraintBottom_toTopOf="@+id/previous_tv"
        app:layout_constraintStart_toEndOf="@+id/imageView4" />

    <ImageView
        android:layout_width="35dp"
        android:layout_height="30dp"
        android:layout_marginTop="6dp"
        android:src="@drawable/sandwich"
        app:layout_constraintEnd_toEndOf="@+id/previous_tv"
        app:layout_constraintStart_toStartOf="@+id/previous_tv"
        app:layout_constraintTop_toBottomOf="@+id/previous_tv" />

    <ImageView
        android:layout_width="40dp"
        android:layout_height="30dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="6dp"
        android:src="@drawable/burger"
        app:layout_constraintBottom_toTopOf="@+id/previous_tv"
        app:layout_constraintEnd_toEndOf="parent" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="30dp"
        android:layout_height="40dp"
        android:layout_marginStart="6dp"
        android:src="@drawable/cherry"
        app:layout_constraintBottom_toTopOf="@+id/previous_tv"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="40dp"
        android:layout_height="30dp"
        android:layout_marginTop="8dp"
        android:src="@drawable/pizza"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.646"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/previous_search"
        android:layout_width="22dp"
        android:layout_height="25dp"
        android:layout_marginEnd="50dp"
        android:src="@drawable/img_clipart2883797"
        app:layout_constraintBottom_toBottomOf="@+id/previous_tv"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/previous_tv" />


    <ImageView
        android:id="@+id/previous_filter"
        android:layout_width="28dp"
        android:layout_height="20dp"
        android:layout_marginStart="12dp"
        android:src="@drawable/img_volume"
        app:layout_constraintBottom_toBottomOf="@+id/previous_search"
        app:layout_constraintStart_toEndOf="@+id/previous_search"
        app:layout_constraintTop_toTopOf="@+id/previous_search" />

    <TextView
        android:id="@+id/previous_tv"
        android:layout_width="262dp"
        android:layout_height="29dp"
        android:layout_marginStart="18dp"
        android:layout_marginTop="46dp"
        android:text="Previous Transactions"
        android:textColor="#000000"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/previous_rV"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="124dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/previous_iv" />

</androidx.constraintlayout.widget.ConstraintLayout>