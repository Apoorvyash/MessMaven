<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.MessMavenDemo"
        tools:targetApi="31">
        <activity
            android:name=".TakeLeaveActivity"
            android:exported="false" />
        <activity
            android:name=".ui.notifications.FeedbackActivity"
            android:exported="false" />
        <activity
            android:name=".ui.home.Announcements"
            android:exported="false" />
        <activity
            android:name=".ui.home.RulesActivity"
            android:exported="false" />
        <activity
            android:name=".ui.dashboard.TransactionDoneActivity"
            android:exported="false" />
        <activity
            android:name=".ui.dashboard.ItemPurchaseActivity"
            android:exported="false" />
        <activity
            android:name=".on_boarding.CreateProfile"
            android:exported="false" />
        <activity
            android:name=".PreviousTransaction"
            android:exported="false" />
        <activity
            android:name=".on_boarding.LoginActivity"
            android:exported="false" />
        <activity
            android:name=".on_boarding.SplashScreen"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="false"
            android:label="@string/app_name" />
        <activity
            android:name="com.journeyapps.barcodescanner.CaptureActivity"
            android:screenOrientation="portrait"
            tools:replace="screenOrientation" />
    </application>

</manifest>